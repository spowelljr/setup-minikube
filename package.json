{
  "name": "minikube-action",
  "version": "0.0.3",
  "private": true,
  "description": "start a kubernetes cluster in github actions",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medyagh/setup-minikube"
  },
  "keywords": [
    "minikube",
    "minikube CI",
    "test aginst kubernetes",
    "test kubernetes",
    "kuberentes CI",
    "kubernetes",
    "kubernetes cluster",
    "github",
    "actions"
  ],
  "author": "medyagh",
  "license": "MIT",
  "dependencies": {
    "@actions/cache": "^3.0.6",
    "@actions/core": "^1.9.1",
    "@actions/exec": "^1.1.1",
    "@actions/tool-cache": "^2.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.24.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^27.0.4",
    "jest": "^29.0.3",
    "jest-circus": "^29.0.3",
    "js-yaml": "^4.1.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.2",
    "typescript": "^4.8.4"
  }
}
